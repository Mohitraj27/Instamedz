<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".nutri_tips"
    android:adjustViewBounds="true">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:scrollbarAlwaysDrawVerticalTrack="true">

        <androidx.constraintlayout.widget.ConstraintLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:adjustViewBounds="true"
                android:scrollbars="vertical"
                android:src="@drawable/health_tips_nutri"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/Nutri_exc_button"
                android:layout_width="510dp"
                android:layout_height="132dp"
                android:layout_marginStart="104dp"
                android:alpha="0"
                android:layout_marginEnd="104dp"
                android:onClick="Nutri_exc_dialog"

                app:layout_constraintBottom_toBottomOf="@+id/imageView5"
                app:layout_constraintEnd_toEndOf="@+id/imageView5"
                app:layout_constraintHorizontal_bias="0.43"
                app:layout_constraintStart_toStartOf="@+id/imageView5"
                app:layout_constraintTop_toTopOf="@+id/imageView5"
                app:layout_constraintVertical_bias="0.235" />

            <Button
                android:id="@+id/Nutri_sleep_button"
                android:layout_width="510dp"
                android:layout_height="132dp"
                android:alpha="0"
                android:layout_marginStart="104dp"
                android:layout_marginEnd="104dp"
                android:onClick="Nutri_sleep_dialog"

                app:layout_constraintBottom_toBottomOf="@+id/imageView5"
                app:layout_constraintEnd_toEndOf="@+id/imageView5"
                app:layout_constraintHorizontal_bias="0.46"
                app:layout_constraintStart_toStartOf="@+id/imageView5"
                app:layout_constraintTop_toTopOf="@+id/imageView5"
                app:layout_constraintVertical_bias="0.423" />

            <Button
                android:id="@+id/Nutri_hydrate_button"
                android:alpha="0"
                android:layout_width="510dp"
                android:layout_height="132dp"
                android:layout_marginStart="104dp"

                android:layout_marginEnd="104dp"
                android:onClick="Nutri_hydrate_dialog"

                app:layout_constraintBottom_toBottomOf="@+id/imageView5"
                app:layout_constraintEnd_toEndOf="@+id/imageView5"
                app:layout_constraintHorizontal_bias="0.529"
                app:layout_constraintStart_toStartOf="@+id/imageView5"
                app:layout_constraintTop_toTopOf="@+id/imageView5"
                app:layout_constraintVertical_bias="0.614" />

            <Button
                android:id="@+id/Nutri_addiction_button"
                android:alpha="0"
                android:layout_width="510dp"
                android:layout_height="132dp"
                android:layout_marginStart="104dp"

                android:layout_marginEnd="104dp"
                android:onClick="Nutri_addiction_dialog"

                app:layout_constraintBottom_toBottomOf="@+id/imageView5"
                app:layout_constraintEnd_toEndOf="@+id/imageView5"
                app:layout_constraintHorizontal_bias="0.431"
                app:layout_constraintStart_toStartOf="@+id/imageView5"
                app:layout_constraintTop_toTopOf="@+id/imageView5"
                app:layout_constraintVertical_bias="0.806" />

            <Button
                android:id="@+id/Nutri_junk_button"
                android:alpha="0"
                android:layout_width="510dp"
                android:layout_height="132dp"
                android:onClick="Nutri_junk_dialog"


                app:layout_constraintBottom_toBottomOf="@+id/imageView5"
                app:layout_constraintEnd_toEndOf="@+id/imageView5"
                app:layout_constraintHorizontal_bias="0.535"
                app:layout_constraintStart_toStartOf="@+id/imageView5"
                app:layout_constraintTop_toTopOf="@+id/imageView5"
                app:layout_constraintVertical_bias="0.994" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>