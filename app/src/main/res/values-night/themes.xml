<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Instamedz" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#E18D09</item>
        <item name="colorPrimaryVariant">#144970</item>
        <item name="colorOnPrimary">#4CAF50</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">#E18D09</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" >?attr/actionBarTheme</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Instamedz.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Instamedz.AppBarOverlay"
        parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Instamedz.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>